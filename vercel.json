{
    "builds": [
        {
            "src": "FPPMS/FPPMS/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.8"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "FPPMS/FPPMS/wsgi.py"
        }
    ]
}